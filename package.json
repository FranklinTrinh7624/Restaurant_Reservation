  "license": "MIT",
  "devDependencies": {
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "concurrently": "^3.5.1",
    "connect-mongodb-session": "^3.1.1",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "express-session": "^1.17.3",
    "mongodb": "^4.12.0",
    "mongoose": "^6.7.2",
    "path": "^0.12.7"
  }
}
